<h1>Userlists#index</h1>

<% if @q.present? %>
  <h2>Search Results:</h2>
<% end %>
<% if @users.any? %>
  <ul>
    <% @users.each do |user| %>
      <li>
        <%= link_to user.name, "#" %>
        <%= render 'follow_button', user: user %>
      </li>
    <% end %>
  </ul>
<% else %>
  <p>No users found</p>
<% end %>